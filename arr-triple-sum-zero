// my solution
#include <stdio.h>
#include <stdlib.h>

int* createarr(int n){
    int *arr = malloc(n * sizeof(int));
    
    for(int i = 0; i < n; i++){
        scanf("%d", &arr[i]);
    };
    
    return arr;
}

void generatetriplets(int arr[],int n,int x){
    for(int i = 0; i < n; i++){
        for(int j = (i + 1); j < n; j++){
            for(int k = (j + 1); k < n; k++){
                if((arr[i] + arr[j] + arr[k]) == x){
                    printf("%d %d %d are triplets \n", arr[i], arr[j], arr[k]);
                };
            }
        }
    }
}

int main(){
    int n;
    printf("Enter number of elements for array :");
    scanf("%d", &n);
    
    int *arr;
    arr = createarr(n);
    generatetriplets(arr,n,0);
    
    return 0;
}

